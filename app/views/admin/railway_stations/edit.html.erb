<div class="row">
<div class="panel panel-default">
  <div class="panel-heading">
    <h1>Editing Railway Station</h1>
  </div>
  <div class="panel-body">
    <div class="col-md-10">
      <%= render 'form', railway_station: @railway_station, hidden: false %>
    </div>
    <div class="col-md-2">
      <ul class="list-unstyled">
        <li>
          <%= link_to t('common.show'), [:admin, @railway_station], class: "text-info" %>
        </li>
        <li>
          <%= link_to t('common.back'), admin_railway_stations_path %>
        </li>
      </ul>
    </div>
  </div>
  </div>
</div>
