<%= form_for @search, url: { action: 'show' } do |f| %>
  <div class="field">
    <%= f.label :start_station_id, t('.start_station') %>
    <%= f.collection_select :start_station_id, RailwayStation.all, :id, :title %>
  </div>

  <div class="field">
    <%= f.label :end_station_id, t('.end_station') %>
    <%= f.collection_select :end_station_id, RailwayStation.all, :id, :title %>
  </div>

  <div class="actions">
    <%= f.submit t('.submit') %>
  </div>
<% end %>

